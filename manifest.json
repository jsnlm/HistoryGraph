{
  "name": "History Graph",
  "version": "1.5",
  "description": "Reads your history, and shows the top ten pages you go to by typing the URL.",
  "permissions": [
    "history", "tabs", "webNavigation"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-1.11.1.min.js", "d3/d3.v3.js", "logic.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_popup": "GraphView.html",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}